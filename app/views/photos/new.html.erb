<h2>New Photo</h2>

<%= form_for @photo, :url => photos_path do |f| %>

  <p>
    <%= f.label :image %>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.label :content %>
    <%= f.text_area :content %>
  </p>

  <p>
    <%= f.submit %>
  </p>

  <p>
    <%= link_to 'Back', photos_path %>
  </p>

<% end %>